{
  "@@locale": "en",
  "signUpTitle": "Sign up for {nameOfTheApp} {when}",
  "@signUpTitle": {
    "description": "The title people see when they open the app for the first time.",
    "placeholders": {
      "nameOfTheApp": {
        "type": "String",
        "example": "TikTong"
      },
      "when": {
        "type": "DateTime",
        "format": "MMMEd",
        "isCustomDateFormat": true
      }
    }
  },
  "loginTitle": "Log into your {nameOfTheApp} account",
  "@loginTitle": {
    "description": "The title people see when they open the log in screen",
    "placeholders": {
      "nameOfTheApp": {
        "type": "String",
        "example": "TikTong"
      }
    }
  },
  "signUpSubtitle": "Create a profile, follow other accounts, make your own {videoCount, plural, =0{no videos} =1{vdieo} other{videos}}, and more.",
  "emailPasswordButton": "Use email & password",
  "appleButton": "Continue with Apple",
  "logIn": "Log in {gender, select, male{sir} female{madam} other{human}}",
  "likeCount": "{value}",
  "@likeCount": {
    "description": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentCount": "{value}",
  "@commentCount": {
    "description": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentTitle": "{value} {value2, plural, =1{comment} other{comments}}",
  "@commentTitle": {
    "description": "Anything you want",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  }
}
